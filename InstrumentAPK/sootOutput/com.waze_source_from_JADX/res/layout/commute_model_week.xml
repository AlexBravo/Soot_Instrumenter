<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/com.waze" app:background="@color/settings_bg_titles" app:layout_width="match_parent" app:layout_height="match_parent">
    <com.waze.view.title.TitleBar app:id="@+id/theTitleBar" app:layout_width="match_parent" app:layout_height="wrap_content" />
    <ScrollView app:background="@color/settings_bg_titles" app:fadingEdge="UNKNOWN_DATA_0x0" app:layout_width="match_parent" app:layout_height="match_parent" app:layout_below="@+id/theTitleBar" app:overScrollMode="ifContentScrolls">
        <LinearLayout style="@style/settingsLayout"
            <LinearLayout style="@style/settingsSection">
                <com.waze.settings.SettingsTitleText app:id="@+id/commuteModelWeekHomeWorkTitle" app:text="Quick Settings" app:position="atThumb" app:icon="@drawable/list_edit_icon" style="@style/settingsTitle" />
                <com.waze.settings.WazeSettingsView app:id="@+id/commuteModelWeekHome" app:position="atThumb" app:settingsType="1" app:icon="@drawable/list_edit_icon" style="@style/settingsSelection" />
                <com.waze.settings.WazeSettingsView app:id="@+id/commuteModelWeekWork" app:position="aboveThumb" app:settingsType="1" app:icon="@drawable/list_edit_icon" style="@style/settingsSelection" />
            </LinearLayout>
            <LinearLayout style="@style/settingsSectionWithTitle">
                <com.waze.settings.SettingsTitleText app:id="@+id/commuteModelWeekDaysTitle" app:text="Settings" style="@style/settingsTitle" />
                <com.waze.settings.WazeSettingsView app:id="@+id/commuteModelWeekDays1" app:position="atThumb" app:settingsType="7" style="@style/settingsSelection" />
                <com.waze.settings.WazeSettingsView app:id="@+id/commuteModelWeekDays2" app:position="floating" app:settingsType="7" style="@style/settingsSelection" />
                <com.waze.settings.WazeSettingsView app:id="@+id/commuteModelWeekDays3" app:position="floating" app:settingsType="7" style="@style/settingsSelection" />
                <com.waze.settings.WazeSettingsView app:id="@+id/commuteModelWeekDays4" app:position="floating" app:settingsType="7" style="@style/settingsSelection" />
                <com.waze.settings.WazeSettingsView app:id="@+id/commuteModelWeekDays5" app:position="floating" app:settingsType="7" style="@style/settingsSelection" />
                <com.waze.settings.WazeSettingsView app:id="@+id/commuteModelWeekDays6" app:position="floating" app:settingsType="7" style="@style/settingsSelection" />
                android:background=&quot;@drawable/item_selector_middle&quot; /&gt;
                <com.waze.settings.WazeSettingsView app:id="@+id/commuteModelWeekDays7" app:position="aboveThumb" app:settingsType="7" style="@style/settingsSelection" />
                <ImageView app:layout_width="match_parent" app:layout_height="wrap_content" app:src="@drawable/cell_shadow_dark" app:scaleType="fitXY" />
            </LinearLayout>
            <com.waze.view.text.WazeTextView app:textSize="15dp" app:textStyle="UNKNOWN_DATA_0x0" app:textColor="@color/BlueDeep" app:id="@+id/commuteModelWeekDaysComment" app:layout_width="match_parent" app:layout_height="wrap_content" app:layout_marginLeft="10dp" app:layout_marginTop="10dp" app:layout_marginRight="10dp" app:layout_marginBottom="10dp" app:text="*Edit your schedule to get accurate requests" style="@style/textOnBg" />
            <com.waze.view.text.WazeTextView app:textSize="15dp" app:textStyle="UNKNOWN_DATA_0x0" app:textColor="@color/BlueDeep" app:gravity="top|bottom|left|right|center_vertical|fill_vertical|center_horizontal|fill_horizontal|center|fill|start|end" app:id="@+id/commuteModelWeekDaysLink" app:paddingBottom="15dp" app:layout_width="match_parent" app:layout_height="wrap_content" app:layout_margin="10dp" app:text="*Provide feedback on this setting" style="@style/textOnBg" />
        </LinearLayout>
    </ScrollView>
    <ImageView app:layout_width="match_parent" app:layout_height="UNKNOWN_DATA_0x601" app:src="@drawable/topbar_shadow" app:scaleType="fitXY" app:layout_below="@+id/theTitleBar" />
</RelativeLayout>
