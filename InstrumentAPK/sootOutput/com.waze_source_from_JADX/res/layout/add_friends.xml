<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/com.waze" app:orientation="vertical" app:layout_width="match_parent" app:layout_height="match_parent">
    <com.waze.view.title.TitleBar app:id="@+id/shareTitle" app:layout_width="match_parent" app:layout_height="wrap_content" />
    <ScrollView app:background="@color/blue_bg" app:layout_width="match_parent" app:layout_height="match_parent" app:layout_below="@+id/shareTitle">
        <LinearLayout app:orientation="vertical" app:layout_width="match_parent" app:layout_height="wrap_content">
            <com.waze.view.text.WazeTextView app:textStyle="UNKNOWN_DATA_0x0" app:gravity="top|bottom|left|right|center_vertical|fill_vertical|center_horizontal|fill_horizontal|center|fill|start|end" app:layout_gravity="top|bottom|left|right|center_vertical|fill_vertical|center_horizontal|fill_horizontal|center|fill|start|end" app:id="@+id/addFriendsBodyText" app:layout_marginLeft="10dp" app:layout_marginTop="10dp" app:layout_marginRight="10dp" app:layout_marginBottom="10dp" app:text="Pick someone up:" style="@style/textOnBg" />
            <com.waze.settings.WazeSettingsView app:id="@+id/AddContactsButton" app:position="UNKNOWN_DATA_0x3" app:settingsType="0" app:icon="@drawable/list_icon_profile" style="@style/settingsSelection" />
            <com.waze.settings.SettingsTitleText app:id="@+id/FriendsAddedYouTitle" app:visibility="gone" app:text="Quick Settings" style="@style/settingsTitle" />
            <LinearLayout app:orientation="vertical" app:id="@+id/FriendsAddedLayout" app:background="#ffffff" app:visibility="gone" app:layout_width="match_parent" app:layout_height="wrap_content" />
            <com.waze.view.text.WazeTextView app:textStyle="UNKNOWN_DATA_0x0" app:layout_gravity="top|bottom|left|right|center_vertical|fill_vertical|center_horizontal|fill_horizontal|center|fill|start|end" app:id="@+id/friendsAddedText" app:visibility="gone" app:layout_marginLeft="10dp" app:layout_marginTop="5dp" app:layout_marginRight="10dp" app:layout_marginBottom="10dp" app:text="Pick someone up:" style="@style/textOnBg" />
            <com.waze.settings.SettingsTitleText app:id="@+id/FriendsSuggestionTitle" app:text="Quick Settings" style="@style/settingsTitle" />
            <LinearLayout app:orientation="vertical" app:id="@+id/FriendsSuggestionLayout" app:background="#ffffff" app:layout_width="match_parent" app:layout_height="wrap_content" />
            <com.waze.settings.SettingsTitleText app:id="@+id/MoreOptions" app:text="Quick Settings" style="@style/settingsTitle" />
            <com.waze.settings.WazeSettingsView app:id="@+id/AddFromFacebook" app:position="UNKNOWN_DATA_0x3" app:settingsType="0" app:icon="@drawable/list_icon_facebook_settings" style="@style/settingsSelection" />
            <com.waze.settings.WazeSettingsView app:id="@+id/SearchWazeUserName" app:visibility="gone" app:position="aboveThumb" app:settingsType="0" app:icon="@drawable/search_icon" style="@style/settingsSelection" />
            <FrameLayout app:background="@color/settings_bg_titles" app:layout_width="match_parent" app:layout_height="UNKNOWN_DATA_0x7f0a0089" app:foreground="@drawable/cell_shadow_dark" />
        </LinearLayout>
    </ScrollView>
    <ImageView app:layout_width="match_parent" app:layout_height="wrap_content" app:src="@drawable/topbar_shadow" app:scaleType="fitXY" app:layout_below="@+id/shareTitle" />
</RelativeLayout>
