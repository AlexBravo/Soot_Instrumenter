            <com.waze.view.text.WazeTextView app:textSize="15sp" app:textColor="@color/Light" app:id="@+id/lblCarTypeDescription" app:visibility="gone" app:layout_width="wrap_content" app:layout_height="wrap_content" app:maxLines="2" app:font_type="1" />
        </LinearLayout>
    </RelativeLayout>
    <ImageView app:layout_width="wrap_content" app:layout_height="wrap_content" app:layout_marginLeft="24dp" app:src="@drawable/arrow_expanded_cell" app:layout_below="@+id/frontContainer" />
</RelativeLayout>
