<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/com.waze" app:layout_width="match_parent" app:layout_height="match_parent">
    <com.waze.view.title.TitleBar app:id="@+id/theTitleBar" app:layout_width="match_parent" app:layout_height="wrap_content" />
    <ScrollView app:fadingEdge="UNKNOWN_DATA_0x0" app:layout_width="match_parent" app:layout_height="match_parent" app:fillViewport="true" app:layout_below="@+id/theTitleBar" app:overScrollMode="ifContentScrolls">
        <LinearLayout style="@style/settingsSection">
            <View app:background="@color/settings_bg_titles" app:layout_width="match_parent" app:layout_height="UNKNOWN_DATA_0x7f0a008a" />
            <com.waze.settings.WazeSettingsView app:id="@+id/settingsAlertsOnRoutePolice" app:layout_height="wrap_content" app:position="floating" app:settingsType="2" style="@style/settingsSelection" />
            <com.waze.settings.WazeSettingsView app:id="@+id/settingsAlertsOnRouteAccident" app:layout_height="wrap_content" app:position="floating" app:settingsType="2" style="@style/settingsSelection" />
            <com.waze.settings.WazeSettingsView app:id="@+id/settingsAlertsOnRouteHazardOnRoad" app:layout_height="wrap_content" app:position="floating" app:settingsType="2" style="@style/settingsSelection" />
            <com.waze.settings.WazeSettingsView app:id="@+id/settingsAlertsOnRouteHazardOnShoulder" app:layout_height="wrap_content" app:position="floating" app:settingsType="2" style="@style/settingsSelection" />
            <com.waze.settings.WazeSettingsView app:id="@+id/settingsAlertsOnRouteWeatherHazard" app:layout_height="wrap_content" app:position="floating" app:settingsType="2" style="@style/settingsSelection" />
            <com.waze.settings.WazeSettingsView app:id="@+id/settingsAlertsOnRouteOtherHazard" app:layout_height="wrap_content" app:position="floating" app:settingsType="2" style="@style/settingsSelection" />
            <com.waze.settings.WazeSettingsView app:id="@+id/settingsAlertsOnRouteSpeedCameras" app:layout_height="wrap_content" app:position="floating" app:settingsType="2" style="@style/settingsSelection" />
            <com.waze.settings.WazeSettingsView app:id="@+id/settingsAlertsOnRouteDangerZone" app:layout_height="wrap_content" app:position="aboveThumb" app:settingsType="2" style="@style/settingsSelection" />
            <com.waze.settings.WazeSettingsView app:id="@+id/settingsAlertsOnRouteHeadlightReminder" app:layout_height="wrap_content" app:position="aboveThumb" app:settingsType="2" style="@style/settingsSelection" />
            <View app:background="@color/blue_bg" app:layout_width="match_parent" app:layout_height="UNKNOWN_DATA_0x1" app:layout_weight="1.0" />
        </LinearLayout>
    </ScrollView>
</RelativeLayout>
