<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:id="@+id/forMarginesOnly" android:layout_width="wrap_content" android:layout_height="wrap_content"
    <RelativeLayout android:id="@+id/placePhotoFrme" android:background="0x106000b" android:padding="4dp" android:layout_width="match_parent" android:layout_height="match_parent">
        <FrameLayout android:background="#92c2d1" android:layout_width="match_parent" android:layout_height="match_parent">
