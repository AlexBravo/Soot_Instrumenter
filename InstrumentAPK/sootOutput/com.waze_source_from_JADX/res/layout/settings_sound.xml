<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/com.waze" app:orientation="vertical" app:layout_width="match_parent" app:layout_height="match_parent">
    <com.waze.view.title.TitleBar app:id="@+id/theTitleBar" app:layout_width="match_parent" app:layout_height="wrap_content" />
    <ScrollView app:fadingEdge="UNKNOWN_DATA_0x0" app:layout_width="match_parent" app:layout_height="match_parent" app:fillViewport="true" app:layout_below="@+id/theTitleBar" app:overScrollMode="ifContentScrolls">
        <LinearLayout style="@style/settingsSection">
            <View app:background="@color/settings_bg_titles" app:layout_width="match_parent" app:layout_height="UNKNOWN_DATA_0x7f0a008a" />
            <com.waze.settings.WazeSettingsView app:id="@+id/settingsSoundDevice" app:visibility="gone" app:position="UNKNOWN_DATA_0x3" app:settingsType="1" style="@style/settingsSelection" />
            <com.waze.settings.WazeSettingsView app:id="@+id/settingsPromptsVolume" app:position="floating" app:settingsType="4" style="@style/settingsSelection" />
            <com.waze.settings.WazeSettingsView app:id="@+id/settingsGeneralNavigationGuidance" app:position="floating" app:settingsType="1" style="@style/settingsSelection" />
            <com.waze.settings.WazeSettingsView app:id="@+id/settingsCustomPrompts" app:position="floating" app:settingsType="0" style="@style/settingsSelection">
                <com.waze.view.text.WazeTextView app:textSize="14sp" app:textColor="#fff" app:gravity="top|bottom|left|right|center_vertical|fill_vertical|center_horizontal|fill_horizontal|center|fill|start|end" app:id="@+id/settingsCustomPromptsNew" app:background="@drawable/ridecard_status_schedualed" app:paddingLeft="10dp" app:paddingTop="4dp" app:paddingRight="10dp" app:paddingBottom="4dp" app:layout_width="wrap_content" app:layout_height="wrap_content" app:text="*New" app:layout_alignParentRight="true" app:font_type="1" />
            </com.waze.settings.WazeSettingsView>
            <com.waze.settings.WazeSettingsView app:id="@+id/settingsGeneralSearchByVoice" app:position="floating" app:settingsType="1" style="@style/settingsSelection" />
            <com.waze.settings.WazeSettingsView app:id="@+id/settingsMainSettingsVoice" app:visibility="gone" app:position="floating" app:settingsType="1" style="@style/settingsSelection" />
            <com.waze.settings.WazeSettingsView app:id="@+id/settingsRoute2Speaker" app:position="floating" app:settingsType="2" style="@style/settingsSelection" />
            <com.waze.settings.WazeSettingsView app:id="@+id/settingsVibrate" app:position="floating" app:settingsType="1" style="@style/settingsSelection" />
            <com.waze.settings.WazeSettingsView app:id="@+id/settingsMuteDuringCalls" app:position="aboveThumb" app:settingsType="2" style="@style/settingsSelection" />
            <FrameLayout app:background="@color/settings_bg_titles" app:layout_width="match_parent" app:layout_height="UNKNOWN_DATA_0x7f0a0089" app:foreground="@drawable/cell_shadow_dark" />
            <View app:background="@color/blue_bg" app:layout_width="match_parent" app:layout_height="UNKNOWN_DATA_0x1" app:layout_weight="1.0" />
        </LinearLayout>
    </ScrollView>
    <ImageView app:layout_width="match_parent" app:layout_height="wrap_content" app:src="@drawable/topbar_shadow" app:scaleType="fitXY" app:layout_below="@+id/theTitleBar" />
</RelativeLayout>
