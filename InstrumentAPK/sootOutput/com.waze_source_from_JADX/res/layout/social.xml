<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/com.waze" app:orientation="vertical" app:layout_width="match_parent" app:layout_height="match_parent">
    <com.waze.view.title.TitleBar app:id="@+id/socialTitle" app:layout_width="match_parent" app:layout_height="wrap_content" />
    <ScrollView app:fadingEdge="UNKNOWN_DATA_0x0" app:layout_width="match_parent" app:layout_height="match_parent" app:layout_below="@+id/socialTitle" app:overScrollMode="always">
        <LinearLayout app:orientation="vertical" app:layout_width="match_parent" app:layout_height="wrap_content">
            <com.waze.settings.SettingsTitleText app:id="@+id/SocialTitleText" app:text="Quick Settings" app:position="atThumb" style="@style/settingsTitle" />
            <com.waze.settings.WazeSettingsView app:id="@+id/socialFacebook" app:position="atThumb" app:settingsType="0" app:icon="@drawable/list_icon_facebook_settings" style="@style/settingsSelection" />
            <com.waze.settings.WazeSettingsView app:id="@+id/socialTwitter" app:position="floating" app:settingsType="0" app:icon="@drawable/list_icon_twitter" style="@style/settingsSelection" />
            <com.waze.settings.WazeSettingsView app:id="@+id/socialLinked" app:position="aboveThumb" app:settingsType="0" app:icon="@drawable/list_icon_linkedin" style="@style/settingsSelection" />
            <com.waze.settings.SettingsTitleText app:id="@+id/SocialChitChatTitleText" app:text="Quick Settings" style="@style/settingsTitle" />
            <com.waze.settings.WazeSettingsView app:id="@+id/enablePing" app:position="atThumb" app:settingsType="2" style="@style/settingsSelection" />
            <com.waze.settings.WazeSettingsView app:id="@+id/enablePM" app:position="aboveThumb" app:settingsType="2" style="@style/settingsSelection" />
            <FrameLayout app:background="@color/settings_bg_note" app:layout_width="match_parent" app:layout_height="wrap_content">
                <View app:layout_gravity="top|bottom|center_vertical|fill_vertical|center|fill" app:background="@drawable/note_shadow" app:layout_width="match_parent" app:layout_height="wrap_content" />
                <com.waze.view.text.WazeTextView app:id="@+id/SocialBodyText" app:text="bla" style="@style/settings_note" />
            </FrameLayout>
            <com.waze.settings.SettingsTitleText app:id="@+id/SocialGroupTitleText" app:text="Quick Settings" style="@style/settingsTitle" />
            <com.waze.settings.WazeSettingsView app:id="@+id/settingsGroupsReports" app:position="atThumb" app:settingsType="1" style="@style/settingsSelection" />
            <com.waze.settings.WazeSettingsView app:id="@+id/settingsGroupsIcons" app:position="aboveThumb" app:settingsType="1" style="@style/settingsSelection" />
            <FrameLayout app:background="@color/settings_bg_titles" app:layout_width="match_parent" app:layout_height="UNKNOWN_DATA_0x7f0a0089" app:foreground="@drawable/cell_shadow_dark" />
        </LinearLayout>
    </ScrollView>
    <ImageView app:layout_width="match_parent" app:layout_height="UNKNOWN_DATA_0x601" app:src="@drawable/topbar_shadow" app:scaleType="fitXY" app:layout_below="@+id/socialTitle" />
</RelativeLayout>
