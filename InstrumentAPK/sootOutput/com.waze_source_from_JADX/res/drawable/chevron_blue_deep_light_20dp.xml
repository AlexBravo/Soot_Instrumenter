<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate android:fromDegrees="45.0" android:toDegrees="45.0" android:pivotX="87.5%" android:pivotY="50%">
            <shape android:shape="rectangle">
                <corners android:radius="20dp" />
                <size android:height="3dp" android:width="9dp" />
                <solid android:color="@color/BlueDeepLight" />
            </shape>
        </rotate>
    </item>
    <item>
        <rotate android:fromDegrees="-45.0" android:toDegrees="-45.0" android:pivotX="87.5%" android:pivotY="50%">
            <shape android:shape="rectangle">
                <corners android:radius="20dp" />
                <size android:height="3dp" android:width="9dp" />
                <solid android:color="@color/BlueDeepLight" />
            </shape>
        </rotate>
    </item>
</layer-list>
